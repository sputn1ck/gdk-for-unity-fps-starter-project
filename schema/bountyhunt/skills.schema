package bountyhunt;

import "bountyhunt/common.schema";

component PlayerSkillComponent {
    id = 450001;
    list<int32> skills = 1;
    command ActivateSkillResponse activate_skill(ActivateSkillRequest);
    event ActivatedSkillEvent activated_skill_event;
}

type ActivateSkillRequest{
    int32 id = 1;
}

type ActivateSkillResponse {
    int32 id = 1;
}

type ActivatedSkillEvent {
    int32 id = 1;
}