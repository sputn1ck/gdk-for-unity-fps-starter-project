package bountyhunt;

import "bountyhunt/common.schema";

component TransformSync {
    id = 490001;

    TransformData transform = 1;
}

component RoomBoundObject {
    id = 490002;

    EntityId room_entity_id = 1;
}

component AnimatorSync {
    id = 490003;

    command AnimatorData request_animator(bountyhunt.Empty);
}

type AnimatorData {
    string clip_name = 1;
    float time = 2;
}

type TransformData {
    bountyhunt.Vector3Float scale = 1;
    bountyhunt.Quaternion rotation = 2;
}